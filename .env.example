# Spec Kit Assistant Configuration

# Remote Server (for distributed swarm deployment)
# Configure these in your local .env file
REMOTE_HOST=your-server.example.com
REMOTE_PORT=22
REMOTE_USER=your-username

# API Keys (never commit these!)
GEMINI_API_KEY=your-gemini-api-key-here
OPENAI_API_KEY=your-openai-api-key-here

# Swarm Configuration
SWARM_MAX_AGENTS=10
SWARM_TIMEOUT=300000

# Git Integration
GIT_AUTO_COMMIT=false
