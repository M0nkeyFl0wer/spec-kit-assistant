{
  "name": "spec-kit-assistant",
  "version": "1.0.0",
  "description": "Intelligent Spec Kit Assistant with Golden Retriever Guide and Agent Swarm Orchestration",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js fetch",
    "fetch": "node src/index.js fetch",
    "dev": "node --watch src/index.js",
    "test": "node --test test/**/*.test.js",
    "test:watch": "jest --watch",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:security": "jest test/security",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build": "node scripts/build.js",
    "setup": "node scripts/setup.js",
    "swarm:deploy": "node src/agents/swarm-manager.js deploy",
    "swarm:monitor": "node src/agents/swarm-manager.js monitor",
    "cloud:setup": "node src/cloud/gcp-setup.js",
    "cloud:optimize": "node src/cloud/cost-optimizer.js",
    "spec:guided": "node src/consultation/guided.js",
    "character:generate": "node src/character/art-generator.js",
    "multimedia:demo": "node src/multimedia/demo-generator.js",
    "voice:generate": "node src/multimedia/voice-synthesis.js"
  },
  "keywords": [
    "spec-kit",
    "ai-assistant",
    "agent-swarm",
    "cloud-integration",
    "guided-development"
  ],
  "author": "M0nkeyFl0wer",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/compute": "^4.7.0",
    "@google-cloud/run": "^1.3.0",
    "axios": "^1.6.0",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "figlet": "^1.7.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.0",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.1",
    "ws": "^8.14.0",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@jest/globals": "^29.7.0",
    "c8": "^10.1.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^28.14.0",
    "eslint-plugin-security": "^2.1.1",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "mock-fs": "^5.5.0",
    "nock": "^14.0.10",
    "nodemon": "^3.0.2",
    "sinon": "^21.0.0",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
